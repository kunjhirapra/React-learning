-- Reset sequences to start from 1
SELECT setval(pg_get_serial_sequence('public.cabins','id'), 1, false) AS cabins_seq_reset;
SELECT setval(pg_get_serial_sequence('public.guests','id'), 1, false) AS guests_seq_reset;
SELECT setval(pg_get_serial_sequence('public.bookings','id'), 1, false) AS bookings_seq_reset;



